{
  "version": 2,
  "name": "passage",
  "builds": [
    {"src": "package.json", "use": "@now/static-build"}
  ],
  "routes": [
    {"src": "/service-worker.js", "headers": {"cache-control": "s-maxage=0"}, "dest": "/service-worker.js"},
    {"src": "/favicons/(.*)", "dest": "/favicons/$1"},
    {"src": "/[^.]*", "dest": "index.html"}
  ]
}
